{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.4.0",
    "babel-preset-gatsby": "0.2.33",
    "dotenv": "8.2.0",
    "gatsby": "2.19.34",
    "gatsby-background-image": "0.10.2",
    "gatsby-image": "2.2.43",
    "gatsby-link": "2.2.30",
    "gatsby-plugin-create-client-paths": "2.1.23",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-mailchimp": "5.1.2",
    "gatsby-plugin-prefetch-google-fonts": "1.4.2",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-sharp": "2.4.8",
    "gatsby-source-contentful": "2.1.92",
    "gatsby-source-filesystem": "2.1.51",
    "gatsby-transformer-sharp": "2.3.17",
    "generate": "0.14.0",
    "intersection-observer": "0.7.0",
    "nuka-carousel": "4.6.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-hook-form": "5.1.3",
    "react-modal": "3.8.2",
    "react-slick": "0.24.0",
    "react-toastify": "5.3.1",
    "reading-time": "1.2.0",
    "sharp": "0.25.1",
    "slick-carousel": "1.8.1",
    "typeface-lato": "0.0.75",
    "typeface-roboto-slab": "1.1.4",
    "typeface-source-sans-pro": "1.1.5",
    "validator": "^12.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "npm run format:src && npm run format:test",
    "format:src": "prettier --write \"src/**/*.js\"",
    "format:test": "prettier --write \"tests/**/*.js\"",
    "gen:icon": "node scripts/build-icon-json.js",
    "heroku-postbuild": "gatsby build",
    "lint": "lint-staged",
    "test": "jest",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch",
    "view:coverage": "open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-preset-gatsby-package": "0.2.17",
    "chalk": "3.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby-plugin-purgecss": "4.0.1",
    "ghooks": "2.0.4",
    "jest": "25.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "react-test-renderer": "16.13.1",
    "svgson": "4.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format:src",
      "yarn format:test",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint; yarn test"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup-test-env.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "./tests/jest-preprocess.js",
      "./tests/loadershim.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/pages/**/*.{js,jsx}",
      "!/static/vendor/jquery/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/image-mock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/__mocks__/style-mock.js"
    }
  },
  "resolutions": {
    "sharp": "0.23.4"
  }
}
