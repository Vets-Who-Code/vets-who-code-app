{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "babel-preset-gatsby": "0.2.0",
    "gatsby": "2.11.7",
    "gatsby-background-image": "0.7.6",
    "gatsby-image": "2.2.3",
    "gatsby-link": "2.2.0",
    "gatsby-plugin-create-client-paths": "2.1.1",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-gtag": "^1.0.11",
    "gatsby-plugin-prefetch-google-fonts": "1.4.2",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-sharp": "2.2.1",
    "gatsby-source-filesystem": "2.1.1",
    "gatsby-transformer-sharp": "2.2.0",
    "generate": "0.14.0",
    "intersection-observer": "0.7.0",
    "nuka-carousel": "4.5.9",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-hook-form": "5.1.3",
    "react-modal": "3.8.2",
    "react-slick": "0.24.0",
    "react-toastify": "5.3.1",
    "sharp": "0.22.1",
    "slick-carousel": "1.8.1",
    "typeface-lato": "0.0.54",
    "typeface-roboto-slab": "0.0.72",
    "typeface-source-sans-pro": "0.0.54",
    "validator": "^12.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format:src": "prettier --write \"src/**/*.js\"",
    "format:test": "prettier --write \"tests/**/*.js\"",
    "format": "npm run format:src && npm run format:test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "lint": "lint-staged",
    "heroku-postbuild": "gatsby build",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@babel/core": "7.5.4",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-preset-gatsby-package": "0.2.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-plugin-purgecss": "4.0.0",
    "ghooks": "2.0.4",
    "jest": "25.2.3",
    "jest-dom": "4.0.0",
    "lint-staged": "9.0.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.13.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format:src",
      "yarn format:test",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint; yarn test"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup-test-env.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "./tests/jest-preprocess.js",
      "./tests/loadershim.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/pages/**/*.{js,jsx}",
      "!/static/vendor/jquery/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/image-mock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/__mocks__/style-mock.js"
    }
  },
  "resolutions": {
    "sharp": "0.22.1"
  }
}
