{
  "name": "vwc-web-app",
  "version": "1.0.0",
  "repository": "Vets-Who-Code/vwc-site",
  "dependencies": {
    "@reach/dialog": "0.2.1",
    "gatsby": "2.0.11",
    "gatsby-image": "2.0.12",
    "gatsby-link": "2.0.5",
    "gatsby-plugin-create-client-paths": "2.0.0",
    "gatsby-plugin-gtag": "^1.0.11",
    "gatsby-plugin-mailchimp": "https://github.com/benjaminhoffman/gatsby-plugin-mailchimp.git#gatsby-v2",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-sharp": "2.0.5",
    "gatsby-source-filesystem": "2.0.1",
    "gatsby-transformer-sharp": "2.1.3",
    "generate": "0.14.0",
    "jest": "^24.8.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-modal": "3.6.1",
    "react-slick": "0.23.1",
    "react-spring": "8.0.2",
    "react-toastify": "5.0.0-rc.6",
    "slick-carousel": "1.8.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "view:coverage": "open coverage/lcov-report/index.html",
    "lint": "lint-staged",
    "heroku-postbuild": "gatsby build"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "10.0.0",
    "babel-jest": "23.6.0",
    "babel-preset-gatsby-package": "0.1.2",
    "eslint": "5.6.0",
    "eslint-plugin-react": "7.11.1",
    "gatsby-plugin-purgecss": "3.1.0",
    "ghooks": "2.0.4",
    "jest-dom": "2.1.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "react-test-renderer": "16.5.2",
    "react-testing-library": "5.2.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "yarn format",
      "git add"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn lint; yarn test"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setup-test-env.js",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "setupFiles": [
      "./tests/jest-preprocess.js",
      "./tests/loadershim.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/pages/**/*.{js,jsx}",
      "!/static/vendor/jquery/dist"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/image-mock.js",
      "\\.(css|scss|sass)$": "<rootDir>/tests/__mocks__/style-mock.js"
    }
  }
}
