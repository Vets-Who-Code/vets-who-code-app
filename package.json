    "disqus-react": "^1.1.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-equals": "3.0.3",
    "framer-motion": "^6.5.1",
    "gray-matter": "^4.0.3",
    "mailchimp-api-v3": "^1.15.0",
    "marked": "^4.0.18",
    "next": "12.2.2",
